# Frontend Environment Variables
# Copy this file to .env.local for local development
# For production, set these in Vercel Dashboard → Settings → Environment Variables

# API URL - Backend endpoint
# Development: Leave empty to use proxy (localhost:3001)
# Production: Set to your backend URL (e.g., https://youman-backend.railway.app)
# Note: Do NOT include /api/v1 in the URL - it's added automatically
VITE_API_URL=

# Example for production:
# VITE_API_URL=https://youman-backend.railway.app

# Example for custom domain:
# VITE_API_URL=https://api.youman.droidver130.com

